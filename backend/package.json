{
  "name": "sparksupport-backend",
  "version": "1.0.0",
  "description": "SparkSupport Backend API with Express.js and Prisma",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma db push --force-reset && npm run db:seed"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@slack/events-api": "^3.0.1",
    "@slack/web-api": "^7.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "resend": "^6.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "msw": "^2.10.5",
    "nodemon": "^3.0.2",
    "prisma": "^6.14.0"
  },
  "keywords": [
    "express",
    "api",
    "support",
    "tickets",
    "jwt"
  ],
  "author": "SparkSupport",
  "license": "MIT",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
